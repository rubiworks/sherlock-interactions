{
    "component-type": "fill-in-the-blank",
    "results-mode": "reflective | subjective",
    "component-style": "text | drag",
    "prompt-body": "This is a sample prompt for the user to answer.",
    // The gaps in the content array indicate a blank for the user to fill out.
    // There should be # of content - 1 answers
    // A sentence can't end needing an answer. Add a period if it happens.
    "content": [
        "The cat jumped over the ",
        ". The cow ran away with the ",
        "."
    ],
    //correct answers are in order of the answers array
    "answers": [
        "moon",
        "spoon"
    ],
    // when using drag as a component-style, these can add to the pool of available answers
    "answers-fake": ["balloon", "night"],
    "text-valid": "", // if the correct words/blanks are selected
    "text-invalid": "", // if the incorrect words/blanks are selected
    "tooltip": {
        "text": "",
        "color": ""
    }
}